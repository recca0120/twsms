{
    "name": "recca0120/twsms",
    "description": "台灣簡訊 twsms api client",
    "keywords": ["twsms", "sms", "taiwan", "laravel", "notification"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/psr7": "^1.4",
        "nesbot/carbon": "^1.22",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.2",
        "php-http/httplug": "^1.1",
        "php-http/message": "^1.5"
    },
    "require-dev": {
        "illuminate/support": "^5.1",
        "mockery/mockery": "~0.9.4|~1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "~4.8|~5.4|~6.1|~7.0"
    },
    "autoload": {
        "psr-4": {
            "Recca0120\\TwSMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recca0120\\TwSMS\\Tests\\": "tests/"
        }
    },
    "suggest": {
       "illuminate/notifications": "If you need Laravel Notification"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Recca0120\\TwSMS\\TwSMSServiceProvider"
            ]
        }
    }
}
